{
	"name": "zenify/doctrine-behaviors",
	"description": "Port of KnpLabs/DoctrineBehaviors to Nette",
	"license": "MIT",
	"authors": [
		{ "name": "Tomáš Votruba", "homepage": "http://tomasvotruba.cz" }
	],
	"require": {
		"php": ">=5.5",
		"knplabs/doctrine-behaviors": "~1.0.2",
		"kdyby/events": "~2.0"
	},
	"require-dev": {
		"kdyby/doctrine": "~2.0",
		"nette/bootstrap": "~2.2",
		"nette/di": "~2.2",
		"nette/mail": "~2.2",
		"nette/safe-stream": "~2.2",
		"nette/security": "~2.2",
		"tracy/tracy": "~2.0",
		"zenify/coding-standard": "~3.0",
		"jakub-onderka/php-parallel-lint": "~0.8",
		"phpunit/phpunit": "~4.4"
	},
	"autoload": {
		"psr-4": {
			"Zenify\\DoctrineBehaviors\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Zenify\\DoctrineBehaviors\\Tests\\": "tests"
		}
	}
}
